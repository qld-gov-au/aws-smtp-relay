<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
<suppress>
   <notes><![CDATA[
   file name: commons-cli-1.5.0.jar apache:james is a false positive
   ]]></notes>
   <packageUrl regex="true">^pkg:maven/commons\-cli/commons\-cli@.*$</packageUrl>
   <cpe>cpe:/a:apache:james</cpe>
</suppress>
<suppress>
   <notes><![CDATA[
   We are not using Eclipse.
   ]]></notes>
   <packageUrl regex="true">^pkg:maven/org[.]eclipse[.]angus/.*$</packageUrl>
   <cpe>cpe:/a:eclipse:eclipse_ide</cpe>
</suppress>
<suppress>
   <notes><![CDATA[
   We are not using Eclipse.
   ]]></notes>
   <packageUrl regex="true">^pkg:maven/jakarta[.](activation|mail)/jakarta[.](activation|mail)*-api@.*$</packageUrl>
   <cpe>cpe:/a:eclipse:eclipse_ide</cpe>
</suppress>
<suppress>
   <notes><![CDATA[
   Most Apache Commons are not commons-net
   ]]></notes>
   <packageUrl regex="true">^pkg:maven/commons-[^n].*$</packageUrl>
   <cpe>cpe:/a:apache:commons_net</cpe>
</suppress>
<suppress>
   <notes><![CDATA[
   Jackson versioning is not org.json versioning.
   ]]></notes>
   <packageUrl regex="true">^pkg:maven/com[.]fasterxml[.]jackson[.]core/.*$</packageUrl>
   <cpe>cpe:/a:json-java_project:json-java</cpe>
</suppress>
</suppressions>
